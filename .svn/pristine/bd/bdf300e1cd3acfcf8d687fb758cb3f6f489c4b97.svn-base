#sdm-bootstrap-menu.navbar-default .navbar-brand {
    color: rgba(119, 119, 119, 1);
}
#sdm-bootstrap-menu.navbar-default {
    font-size: 15px;
    font-weight: bold;
    /* background-color: rgba(248, 248, 248, 1); */
    /* background-color: rgba(51, 153, 255, 0.28);*/
    background-color: #d3d3d3;
    border-width: 1px;
    border-radius: 4px;
}
#sdm-bootstrap-menu.navbar-default .navbar-nav>li>a {
    color: rgba(119, 119, 119, 1);
    background-color: rgba(248, 248, 248, 0); 
    
}
#sdm-bootstrap-menu.navbar-default .navbar-nav>li>a:hover,
#sdm-bootstrap-menu.navbar-default .navbar-nav>li>a:focus {
    color: rgba(51, 51, 51, 1);
    background-color: rgba(248, 248, 248, 0);
}
#sdm-bootstrap-menu.navbar-default .navbar-nav>.active>a,
#sdm-bootstrap-menu.navbar-default .navbar-nav>.active>a:hover,
#sdm-bootstrap-menu.navbar-default .navbar-nav>.active>a:focus {
    color: rgba(85, 85, 85, 1);
    background-color: rgba(231, 231, 231, 1);
}
#sdm-bootstrap-menu.navbar-default .navbar-toggle {
    border-color: #ddd;
}
#sdm-bootstrap-menu.navbar-default .navbar-toggle:hover,
#sdm-bootstrap-menu.navbar-default .navbar-toggle:focus {
    background-color: #ddd;
}
#sdm-bootstrap-menu.navbar-default .navbar-toggle .icon-bar {
    background-color: #888;
}
#sdm-bootstrap-menu.navbar-default .navbar-toggle:hover .icon-bar,
#sdm-bootstrap-menu.navbar-default .navbar-toggle:focus .icon-bar {
    background-color: #888;
}